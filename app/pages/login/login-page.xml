<Page class="page" navigatingTo="onNavigatingTo" 
    xmlns="http://schemas.nativescript.org/tns.xsd">
    <ActionBar class="action-bar" title="Bitwarden">
        <ActionItem tap="{{submit}}" text="Log In"></ActionItem>
    </ActionBar>
    <StackLayout>
        <Label text="Email"></Label>
        <TextField text="{{email}}" keyboardType="email" />
        <Label text="Master Password"></Label>
        <TextField text="{{masterPassword}}" secure="true" />
    </StackLayout>
</Page>
