{
  "nativescript": {
    "id": {
      "ios": "com.8bit.bitwarden",
      "android": "com.x8bit.bitwarden"
    },
    "tns-ios": {
      "version": "5.2.1"
    },
    "tns-android": {
      "version": "5.2.1"
    }
  },
  "author": "8bit Solutions LLC",
  "homepage": "https://bitwarden.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitwarden/mobile"
  },
  "license": "GPL-3.0",
  "description": "The Bitwarden mobile app vault.",
  "scripts": {
    "sub:init": "git submodule update --init --recursive",
    "sub:update": "git submodule update --remote",
    "sub:pull": "git submodule foreach git pull",
    "postinstall": "npm run sub:init",
    "lint": "tslint \"app/**/*.ts\"",
    "clean": "rimraf ./platforms",
    "android": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tns run android --bundle",
    "ios": "tns run ios --bundle",
    "android:sim": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tns run android --bundle --emulator",
    "ios:sim": "tns run ios --bundle --emulator",
    "android:debug": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tns debug android --bundle",
    "ios:debug": "tns debug ios --bundle",
    "android:debug:sim": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tns debug android --bundle --emulator",
    "ios:debug:sim": "tns debug ios --bundle --emulator"
  },
  "dependencies": {
    "lowdb": "1.0.0",
    "lunr": "2.3.3",
    "nativescript-bottombar": "^4.0.0-rc1.2",
    "nativescript-bw-crypto": "file:plugins/bw-crypto/publish/package/nativescript-bw-crypto-1.0.0.tgz",
    "nativescript-bw-secure-storage": "file:plugins/bw-secure-storage/publish/package/nativescript-bw-secure-storage-1.0.0.tgz",
    "papaparse": "4.6.0",
    "reflect-metadata": "~0.1.8",
    "rxjs": "~6.3.0",
    "tldjs": "2.3.1",
    "tns-core-modules": "^5.2.2",
    "url": "0.11.0",
    "zone.js": "^0.8.26",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@types/lowdb": "^1.0.5",
    "@types/node": "8.0.19",
    "@types/papaparse": "^4.5.4",
    "@types/tldjs": "^2.3.0",
    "@types/zxcvbn": "4.4.0",
    "cross-env": "^5.2.0",
    "nativescript-dev-sass": "^1.7.0",
    "nativescript-dev-typescript": "^0.9.0",
    "nativescript-dev-webpack": "^0.18.5",
    "rimraf": "^2.6.2",
    "tns-platform-declarations": "^5.2.2",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "xcode": "git+https://git@github.com/kspearrin/cordova-node-xcode.git"
  }
}
